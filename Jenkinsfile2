	 
pipeline {
          agent any
           options {
		timestamps ()
	    } 
	
	stages{
		     stage('Create docker'){
			
	                  steps{
						                sh '''
								docker-compose up -d
								sleep 300
								npm install
								npm run build
								(npm run start&)
								  '''
							    	}
	         }	    
	}	    
	 
}    
		   
